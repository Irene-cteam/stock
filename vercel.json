{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
        "rewrites": [
      { "source": "/(.*)", "destination": "/api/index" }
    ]

}
